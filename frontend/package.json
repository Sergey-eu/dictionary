{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@corets/use-throttle": "^0.2.2",
    "classnames": "^2.3.2",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "sass": "^1.55.0",
    "typed-scss-modules": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:jest && npm run test:cy",
    "test:cy": "start-server-and-test 3000 'cypress run'",
    "test:jest": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "lint": "npx eslint --fix src",
    "format": "prettier --ignore-path .gitignore --write src/**/*.{ts,tsx}",
    "css": "typed-scss-modules src --watch --nameFormat none --exportType default"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "cypress": "^10.8.0",
    "eslint": "^8.24.0",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.3"
  }
}
